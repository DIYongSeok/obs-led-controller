{
    "include": ["./src/client/**/*.tsx"],
    "exclude": [
        "node_modules"
    ],
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "es6",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": false
    }  
}